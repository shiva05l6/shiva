pipeline
{
  agent any
  stages
  {
    stage('clone')
    {
      steps
      {
        git 'https:/github.com/shiva05l6/shiva.git'
      }
    }
    stage('build')
    {
      steps
      {
        sh 'javac hello.java'
      }
    }
    stage ('run')
    {
      steps
      {
        sh'java hello'
      }
    }
  }
}
